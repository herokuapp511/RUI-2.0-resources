<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:androidprv="http://schemas.android.com/apk/prv/res/android" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <stroke android:width="1.33dp" android:color="?attr/colorTintControlNormal"/>
            <padding android:left="3dp" android:top="3dp" android:right="3dp" android:bottom="3dp"/>
            <corners android:radius="14dp"/>
            <smooth-rect/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="1.33dp" android:color="@android:color/transparent"/>
            <padding android:left="3dp" android:top="3dp" android:right="3dp" android:bottom="3dp"/>
            <corners android:radius="14dp"/>
            <smooth-rect/>
        </shape>
    </item>
</selector>
