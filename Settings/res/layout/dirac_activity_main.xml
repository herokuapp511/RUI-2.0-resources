            <TextView android:textSize="@dimen/equalizer_tips_text_size" android:textColor="@android:color/black" android:id="@+id/equalizer_tips" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/equalizer_tips_margin_top" android:text="@string/dirac_headset_equalizer" android:layout_below="@+id/divider1" android:fontFamily="sans-serif-medium" android:layout_marginStart="@dimen/equalizer_tips_margin_start_end" android:layout_marginEnd="@dimen/equalizer_tips_margin_start_end"/>
