                    <TextView android:textSize="@dimen/dirac_name_text_size" android:textColor="@android:color/black" android:ellipsize="end" android:id="@+id/tv_dirac_name" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/dirac_name" android:singleLine="true" android:fontFamily="sans-serif-medium" android:textAlignment="viewStart"/>
