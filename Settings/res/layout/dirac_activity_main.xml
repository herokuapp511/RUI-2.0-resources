                <com.coloros.partners.dirac.widget.DiracModeSelectorGallery android:id="@+id/mode_selector" android:clipChildren="false" android:clipToPadding="false" android:layout_width="match_parent" android:layout_height="@dimen/mode_selector_height" android:layout_marginTop="@dimen/mode_selector_margin_top" android:layout_below="@+id/sound_effect_mode" android:layout_centerHorizontal="true"/>
