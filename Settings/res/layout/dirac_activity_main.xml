                    </RelativeLayout>
                    <RelativeLayout android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="1">
                        <TextView android:textSize="@dimen/bass_middle_treble_type_text_size" android:textColor="@color/bass_middle_treble_type_color" android:gravity="center" android:id="@+id/tv_middle_type" android:background="@drawable/dirac_decibel_bg" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/dts_middle" android:singleLine="true" android:includeFontPadding="false" android:layout_centerHorizontal="true" android:textAlignment="center"/>
                    </RelativeLayout>
