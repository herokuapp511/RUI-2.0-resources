                <TextView android:textSize="@dimen/bass_middle_treble_value_text_size" android:textColor="@color/bass_middle_treble_value_color" android:gravity="center" android:id="@+id/tv_middle_value" android:layout_width="@dimen/bass_middle_treble_value_width" android:layout_height="wrap_content" android:layout_marginTop="@dimen/middle_value_margin_top" android:text="@string/dirac_middle_value" android:layout_marginStart="@dimen/bass_middle_treble_value_margin_start"/>
                <TextView android:textSize="@dimen/bass_middle_treble_value_text_size" android:textColor="@color/bass_middle_treble_value_color" android:gravity="center" android:id="@+id/tv_treble_value" android:layout_width="@dimen/bass_middle_treble_value_width" android:layout_height="wrap_content" android:layout_marginTop="@dimen/treble_value_margin_top" android:text="@string/dirac_treble_value" android:layout_marginStart="@dimen/bass_middle_treble_value_margin_start"/>
