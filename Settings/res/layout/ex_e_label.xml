<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:settings="http://schemas.android.com/apk/res/com.android.settings" xmlns:androidprv="http://schemas.android.com/apk/prv/res/android" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:oppo="http://schemas.android.com/apk/res/oppo" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:paddingTop="100dp" android:layout_width="match_parent" android:layout_height="match_parent">
    <TextView android:textSize="@dimen/TD08" android:textStyle="bold" android:textColor="@color/C21" android:id="@+id/e_model" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_centerHorizontal="true"/>
    <LinearLayout android:id="@+id/sar_layout" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="4dp" android:layout_below="@+id/e_model" android:layout_centerHorizontal="true">
        <TextView android:textSize="@dimen/TD07" android:textColor="@color/C21" android:id="@+id/e_sar_tag" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
        <TextView android:textSize="@dimen/TD07" android:textColor="@color/C21" android:id="@+id/e_sar" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginStart="2dp"/>
    </LinearLayout>
    <ImageView android:id="@+id/e_label" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="5dp" android:layout_below="@+id/sar_layout" android:layout_centerHorizontal="true" android:contentDescription="@null"/>
</RelativeLayout>
