<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:settings="http://schemas.android.com/apk/res/com.android.settings" xmlns:androidprv="http://schemas.android.com/apk/prv/res/android" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:oppo="http://schemas.android.com/apk/res/oppo" xmlns:aapt="http://schemas.android.com/aapt" android:gravity="center" android:orientation="vertical" android:id="@android:id/icon" android:background="@drawable/icon_style_icon_bg" android:layout_width="@dimen/icon_size" android:layout_height="@dimen/icon_size" android:forceDarkAllowed="false">
    <LinearLayout android:gravity="center_horizontal" android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content">
        <ImageView android:id="@+id/themestore" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
        <ImageView android:id="@+id/settings" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginStart="@dimen/icon_style_image_margin"/>
    </LinearLayout>
    <LinearLayout android:gravity="center_horizontal" android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/icon_style_image_margin">
        <ImageView android:id="@+id/contacts" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
        <ImageView android:id="@+id/music" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginStart="@dimen/icon_style_image_margin"/>
    </LinearLayout>
</LinearLayout>
