<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:settings="http://schemas.android.com/apk/res/com.android.settings" xmlns:androidprv="http://schemas.android.com/apk/prv/res/android" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:oppo="http://schemas.android.com/apk/res/oppo" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:paddingLeft="@dimen/regulatory_item_padding" android:paddingRight="@dimen/regulatory_item_padding" android:layout_width="match_parent" android:layout_height="wrap_content">
    <TextView android:id="@+id/title" android:text="@string/ksa_country" style="@style/RegulatoryTitleStyle"/>
    <LinearLayout android:gravity="center_horizontal" android:orientation="vertical" android:background="@drawable/color_regulatory_dash_line_rect" android:paddingTop="10dp" android:paddingBottom="10dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/regulatory_content_margin_top">
        <TextView android:textSize="15sp" android:textStyle="bold" android:textColor="@android:color/black" android:id="@+id/citc_tv" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="CITC"/>
        <LinearLayout android:layout_gravity="center_horizontal" android:orientation="horizontal" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layoutDirection="ltr">
            <TextView android:textSize="15sp" android:textStyle="bold" android:textColor="@android:color/black" android:id="@+id/number_tag" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="6dp" android:text="@string/sabic_regulatory"/>
            <TextView android:textSize="15sp" android:textStyle="bold" android:textColor="@android:color/black" android:id="@+id/number" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="6dp"/>
            <TextView android:textSize="15sp" android:textStyle="bold" android:textColor="@android:color/black" android:id="@+id/number_right_parentheses" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="6dp" android:text=")"/>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
