<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:settings="http://schemas.android.com/apk/res/com.android.settings" xmlns:androidprv="http://schemas.android.com/apk/prv/res/android" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:oppo="http://schemas.android.com/apk/res/oppo" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:paddingLeft="@dimen/regulatory_item_padding" android:paddingRight="@dimen/regulatory_item_padding" android:layout_width="match_parent" android:layout_height="wrap_content">
    <TextView android:id="@+id/title" android:text="@string/thailand_country" style="@style/RegulatoryTitleStyle"/>
    <LinearLayout android:orientation="vertical" android:background="@drawable/color_regulatory_solid_line_rect" android:paddingTop="7dp" android:paddingBottom="9dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/regulatory_content_margin_top">
        <ImageView android:layout_gravity="center_horizontal" android:id="@+id/ic_thailand_regulatory" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/ic_thailand_regulatory"/>
        <LinearLayout android:gravity="center_vertical" android:layout_gravity="center_horizontal" android:orientation="horizontal" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="2dp" android:layoutDirection="ltr">
            <TextView android:textSize="16sp" android:textColor="@android:color/black" android:id="@+id/number_prefix" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/thailand_regulatory"/>
            <TextView android:textSize="16sp" android:textColor="@android:color/black" android:id="@+id/number" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
