<?xml version="1.0" encoding="utf-8"?>
<settings-config-ringtone>
    <group name="ATT" type="sim">
        <ringtones>
            <item name="AT&amp;T City lights" default_ringtone="true" default_alarm="true"/>
            <item name="AT&amp;T Firefly"/>
            <item name="AT&amp;T Now or never"/>
        </ringtones>
        <notifications>
            <item name="AT&amp;T Chat"/>
            <item name="AT&amp;T Chat alert" default_notification="true"/>
            <item name="AT&amp;T Chat alert out"/>
            <item name="AT&amp;T Email receive"/>
            <item name="AT&amp;T Email send"/>
        </notifications>
        <apps>
            <item name="com.google.android.calendar" notification_name="AT&amp;T Email receive"/>
            <item name="com.google.android.gm" notification_name="AT&amp;T Email receive"/>
        </apps>
    </group>
    <group name="UNEFON" type="sim">
        <ringtones>
            <item name="UNEFON_RINGTONE" default_ringtone="true" default_alarm="true"/>
        </ringtones>
    </group>
</settings-config-ringtone>
