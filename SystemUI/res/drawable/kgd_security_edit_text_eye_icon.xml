    <item android:state_checked="true" android:id="@+id/on" android:drawable="@drawable/kgd_security_edit_text_eye_icon_on"/>
    <item android:id="@+id/off" android:drawable="@drawable/kgd_security_edit_text_eye_icon_off"/>
