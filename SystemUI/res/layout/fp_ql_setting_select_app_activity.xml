<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:androidprv="http://schemas.android.com/apk/prv/res/android" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:layout_width="match_parent" android:layout_height="match_parent">
    <androidx.viewpager2.widget.ViewPager2 android:id="@+id/viewpager" android:visibility="invisible" android:clipToPadding="false" android:layout_width="match_parent" android:layout_height="match_parent"/>
    <LinearLayout android:gravity="center" android:layout_gravity="bottom" android:orientation="vertical" android:id="@+id/loading_layout" android:background="@drawable/color_list_preference_bg" android:visibility="visible" android:layout_width="match_parent" android:layout_height="match_parent">
        <com.color.support.widget.ColorLoadingView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@null" style="?attr/colorLoadingViewLargeStyle"/>
        <TextView android:textAppearance="?android:attr/textAppearanceSmall" android:paddingTop="@dimen/Keyguard_fp_ql_loading_layout_padding_top" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/fingerprint_ql_select_loading" android:singleLine="true"/>
    </LinearLayout>
    <color.support.design.widget.ColorAppBarLayout android:id="@+id/appbar_layout" android:background="@null" android:layout_width="match_parent" android:layout_height="wrap_content" app:layout_behavior="@string/appbar_scale_behavior">
        <color.support.v7.widget.Toolbar android:id="@+id/toolbar" android:background="@drawable/fp_ql_toolbar_bg" android:paddingTop="@dimen/toolbar_margin_top" android:layout_width="match_parent" android:layout_height="wrap_content" android:minHeight="@dimen/toolbar_min_height" app:colorLayoutScrollFlags="3"/>
        <RelativeLayout android:id="@+id/fp_ql_menu_panel" android:background="@color/colorWhite" android:paddingTop="@dimen/Keyguard_fp_ql_menu_panel_margin_top" android:focusable="true" android:visibility="invisible" android:clickable="true" android:layout_width="match_parent" android:layout_height="wrap_content">
            <TextView android:textSize="@dimen/font_size_TD05" android:textColor="@color/color_preference_title_color_selected" android:id="@+id/fp_ql_menu_panel_tips" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/fingerprint_ql_setting_preview_tips" android:layout_alignParentTop="true" android:lineSpacingExtra="@dimen/font_size_TD01" android:alpha="0.55" android:fontFamily="OPPOSans2.0" android:textDirection="locale" android:textAlignment="viewStart" android:layout_marginStart="@dimen/common_margin" style="?android:attr/textAppearanceMedium"/>
            <ImageView android:id="@+id/fp_ql_menu_panel_fingerprint_icon" android:layout_width="@dimen/Keyguard_fp_ql_menu_panel_fingerprint_icon_size" android:layout_height="@dimen/Keyguard_fp_ql_menu_panel_fingerprint_icon_size" android:layout_marginBottom="30dp" android:src="@drawable/kgd_osfingerprint_fadein_23" android:layout_above="@+id/fp_ql_menu_panel_divider_line" android:layout_centerHorizontal="true" android:foregroundGravity="center_horizontal" app:tint="@color/color_fp_icon_filter"/>
            <androidx.recyclerview.widget.RecyclerView android:scrollbarStyle="outsideOverlay" android:id="@+id/fp_ql_menu_panel_recycle_view" android:scrollbars="none" android:fadingEdge="none" android:clipChildren="false" android:layout_width="@dimen/kgd_fp_ql_pre_recycler_view_width" android:layout_height="@dimen/kgd_fp_ql_pre_recycler_view_height" android:layout_below="@+id/fp_ql_menu_panel_tips" android:layout_centerHorizontal="true" android:contentDescription="@string/fingerprint_ql_preview" android:overScrollMode="never" android:backgroundTint="#888"/>
            <View android:layout_gravity="center_horizontal" android:id="@+id/fp_ql_menu_panel_divider_line" android:background="@color/color_preference_title_color_selected" android:layout_width="match_parent" android:layout_height="@dimen/divider_background_height" android:layout_marginLeft="@dimen/common_margin" android:layout_marginTop="@dimen/Keyguard_fp_ql_menu_panel_devider_line_margin_top" android:layout_marginRight="@dimen/common_margin" android:layout_marginBottom="@dimen/Keyguard_fp_ql_menu_panel_devider_line_margin_bottom" android:layout_below="@+id/fp_ql_menu_panel_recycle_view" android:alpha="0.1" android:forceDarkAllowed="false"/>
        </RelativeLayout>
        <color.support.design.widget.ColorTabLayout android:id="@+id/tab_layout" android:visibility="invisible" style="@style/ColorMiddleTabLayoutStyle"/>
    </color.support.design.widget.ColorAppBarLayout>
</androidx.coordinatorlayout.widget.CoordinatorLayout>
