<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android" xmlns:aapt="http://schemas.android.com/aapt" android:color="@color/lighter_gray">
    <item>
        <inset android:insetLeft="0dp" android:insetRight="0dp" android:insetTop="8dp" android:insetBottom="8dp">
            <shape android:shape="rectangle">
                <corners android:radius="16dp"/>
                <stroke android:width="1dp" android:color="?^attr-private/opacityListDivider"/>
                <solid android:color="?attr/colorBackgroundFloating"/>
            </shape>
        </inset>
    </item>
</ripple>
