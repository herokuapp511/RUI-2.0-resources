<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:gravity="left|right|center_vertical|center_horizontal|fill_horizontal|center" android:id="@android:id/background">
        <shape android:tint="@android:color/primary_text_material_light" android:shape="rectangle">
            <corners android:radius="@dimen/notification_progress_bar_radius_material"/>
            <size android:height="0x1050213"/>
            <solid android:color="0x106024e"/>
        </shape>
    </item>
    <item android:gravity="left|right|center_vertical|center_horizontal|fill_horizontal|center" android:id="@android:id/secondaryProgress">
        <scale android:scaleWidth="100%">
            <shape android:tint="@color/oplus_notification_progress_color" android:shape="rectangle">
                <corners android:radius="@dimen/notification_progress_bar_radius_material"/>
                <size android:height="0x1050213"/>
                <solid android:color="0x106024e"/>
            </shape>
        </scale>
    </item>
    <item android:gravity="left|right|center_vertical|center_horizontal|fill_horizontal|center" android:id="@android:id/progress">
        <scale android:scaleWidth="100%">
            <shape android:tint="@color/oplus_notification_progress_color" android:shape="rectangle">
                <corners android:radius="@dimen/notification_progress_bar_radius_material"/>
                <size android:height="0x1050213"/>
                <solid android:color="@android:color/white"/>
            </shape>
        </scale>
    </item>
</layer-list>
